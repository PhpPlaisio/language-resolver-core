{
  "name": "plaisio/language-resolver-core",
  "description": "PhpPlaisio: Core Language Resolver",
  "keywords": [
    "PhpPlaisio",
    "Language Resolver",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "plaisio/babel-core": "^1.0",
    "plaisio/kernel": "^1.0",
    "plaisio/language-resolver": "^1.0"
  },
  "require-dev": {
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\LanguageResolver\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\LanguageResolver\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
