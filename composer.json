{
  "name": "plaisio/language-resolver-core",
  "description": "PhpPlaisio: Core Language Resolver",
  "keywords": [
    "PhpPlaisio",
    "Language Resolver",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "plaisio/babel-core": "^2.2.0",
    "plaisio/kernel": "^3.3.1",
    "plaisio/language-resolver": "^1.4.0",
    "plaisio/request": "^2.0.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.25",
    "plaisio/console-kernel": "^1.2.0",
    "plaisio/request-core": "^2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\LanguageResolver\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\LanguageResolver\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
