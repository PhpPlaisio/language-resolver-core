{
  "name": "plaisio/language-resolver-core",
  "description": "PhpPlaisio: Core Language Resolver",
  "keywords": [
    "PhpPlaisio",
    "Language Resolver",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "plaisio/babel-core": "^2.0.0",
    "plaisio/kernel": "^3.0",
    "plaisio/language-resolver": "^1.2",
    "plaisio/request": "^0.13.1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "^3.0.0-RC4",
    "phpunit/phpunit": "^9.6.3",
    "plaisio/console-kernel": "^1.0.1",
    "plaisio/request-core": "^0.13.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\LanguageResolver\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\LanguageResolver\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
